{
    "repo": "https://github.com/qmk/qmk_firmware",
    "branch": "xap",
    "keyboard": "elpekenin/access",
    "keymap": "elpekenin",
    "path": "$QMK_BUILD",
    "default_compilation": false,
    "operations": [
        {
            "operation": "cp",
            "orig": "keyboards/elpekenin/access/*",
            "dest": "keyboards/elpekenin/access"
        },
        {
            "operation": "cp",
            "orig": "users/elpekenin/*",
            "dest": "users/elpekenin"
        },
        {
            // ellipse
            "operation": "pr",
            "id": 19005
        },
        {
            // tz's OLED
            "operation": "pr",
            "id": 19997
        },
        {
            // 3color eink
            "operation": "pr",
            "id": 20032
        },
        {
            // shutdown hooks
            "operation": "pr",
            "id": 21060
        },
        {
            "operation": "checkout",
            "files": ["drivers/painter/il91874"],
            "repo": "https://github.com/elpekenin/qmk_firmware",
            "branch": "il91874"
        },
        {
            "operation": "diff",
            "file": "il91874.diff"
        },
        {
            "operation": "diff",
            "file": "spi_comms.diff"
        },
        {
            "operation": "diff",
            "file": "generic_rotation.diff"
        },
        {
            // #20645(ish)
            "operation": "diff",
            "file": "native_color_font.diff"
        },
        {
            "operation": "diff",
            "file": "print_no_console.diff"
        },
        {
            "operation": "diff",
            "file": "xap_keytester.diff"
        },
        {
            "operation": "exec",
            "command": "keyboards/elpekenin/access/compile",
            "at": "$QMK_BUILD"
        }
    ],
    "post_compilation": [
        {
            "operation": "exec",
            "command": "cp binaries/* /mnt/d"
        }
    ]
}