{
    "repo": "https://github.com/qmk/qmk_firmware",
    "branch": "xap",
    "keyboard": "elpekenin/access",
    "keymap": "elpekenin",
    "default_compilation": false,
    "operations": [
        {
            "operation": "exec",
            "command": "mkdir -p keyboards/elpekenin",
            "at": "$HOME/.__qmk_build__"
        },
        {
            "operation": "cp",
            "orig": "keyboards/access",
            "dest": "keyboards/elpekenin/access"
        },
        {
            "operation": "cp",
            "orig": "userspace",
            "dest": "users/elpekenin"
        },
        {
            // shutdown hooks
            "operation": "pr",
            "id": 21060

        },
        {
            // tz's OLED
            "operation": "pr",
            "id": 19997
        },
        {
            // 3color eink
            "operation": "pr",
            "id": 20032
        },
        {
            "operation": "checkout",
            "files": ["drivers/painter/il91874"],
            "repo": "https://github.com/elpekenin/qmk_firmware",
            "branch": "il91874"
        },
        {
            "operation": "diff",
            "file": "il91874.diff"
        },
        {
            "operation": "diff",
            "file": "spi_comms.diff"
        },
        {
            "operation": "diff",
            "file": "xap_dynamic_keymap.diff"
        },
        {
            "operation": "diff",
            "file": "generic_rotation.diff"
        },
        {
            // #20645(ish)
            "operation": "diff",
            "file": "native_color_font.diff"
        },
        {
            "operation": "exec",
            "command": "keyboards/elpekenin/access/compile",
            "at": "$HOME/.__qmk_build__"
        },
        {
            "operation": "exec",
            "command": "cp $HOME/.__qmk_build__/*uf2 binaries/"
        }
    ]
}