{
    // WIP
    "repo": "https://github.com/qmk/qmk_firmware",
    "branch": "develop",
    "keyboard": "elpekenin/access",
    "keymap": "elpekenin",
    "default_compilation": false,
    "operations": [
        {
            "operation": "exec",
            "command": "mkdir -p keyboards/elpekenin",
            "at": "$HOME/.__qmk_build__"
        },
        {
            "operation": "cp",
            "orig": "keyboards/access",
            "dest": "keyboards/elpekenin"
        },
        {
            "operation": "exec",
            "command": "mkdir -p users/elpekenin",
            "at": "$HOME/.__qmk_build__"
        },
        {
            "operation": "cp",
            "orig": "userspace",
            "dest": "users/elpekenin"
        },
        {
            "description": "tz's OLED",
            "operation": "pr",
            "id": 19997
        },
        {
            "description": "3-color e-Ink",
            "operation": "pr",
            "id": 20032
        },
        {
            "description": "IL91874",
            "operation": "checkout",
            "files": ["drivers/painter/il91874"],
            "repo": "https://github.com/elpekenin/qmk_firmware",
            "branch": "il91874"
        },
        {
            "description": "Custom compile script",
            "operation": "exec",
            "command": "keyboards/elpekenin/access/compile",
            "at": "$HOME/.__qmk_build__"
        }
    ]
}